APP=newbie

# Database
# --------
DB_NAME=${APP}_local
DB_USER=${APP}_db_user
DB_PASS=${APP}_db_password
DB_DUMP=`dirname $0`/db_dumps
DB_HOST=localhost
DB_PREFIX=wp_

# WordPress
# ---------
WP_LOCALE=nl_NL
WP_PATH=wordpress
WP_USER=${APP}_gebruiker
WP_PASS=${APP}_wachtwoord
WP_MAIL=${APP}@arteveldehs.be
WP_URL=www.${APP}.local